<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <stroke
                android:width="3dp"
                android:color="@color/colorDefaultNoteColor" />
        </shape>
    </item>

    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">

        <shape android:shape="oval">
            <stroke
                android:width="7dp"
                android:color="@color/colorMiscellaneousBackground"/>
        </shape>
    </item>
    <item
        android:bottom="7dp"
        android:left="7dp"
        android:right="7dp"
        android:top="7dp">

        <shape android:shape="oval">
            <solid android:color="@color/colorDefaultNoteColor" />
        </shape>
    </item>
</layer-list>